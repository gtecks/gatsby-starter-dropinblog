    "babel-preset-gatsby": "^0.4.2",
    "emotion-theming": "^10.0.27",
    "env-cmd": "8.0.2",
    "gatsby": "^2.21.33",
    "gatsby-awesome-pagination": "^0.3.5",
    "gatsby-image": "^2.2.41",
    "gatsby-plugin-emotion": "^4.2.1",
    "gatsby-plugin-purgecss": "^4.0.1",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-sass": "^2.1.29",
